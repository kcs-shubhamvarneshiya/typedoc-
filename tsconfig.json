{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "noImplicitAny": false,
    "strictNullChecks": false,
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "typedocOptions": {
    "entryPoints": [
      "src/**/*.tsx",
      "src/*.tsx",
      "src/*.ts",
      "src/**/*.ts"
    ],
    "out": "docs",
    "json": "jsonReport",
    "name": "TypeDoc for VC product-design",
    "commentStyle": "all",
    "categorizeByGroup": "true"
  },
  "include": [
    "src"
  ]
}